{
  "presets": [
    ["env", { "modules": false }]
  ],
  "env": {
    "test": {
      "presets": [
        ["env"]
      ],
      "plugins": [
        "transform-runtime"
      ]
    }
  },
  "ignore": [
    "node_modules/**"
  ],
  "plugins": [
    "lodash",
    "external-helpers",
    "transform-function-bind",
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
} 